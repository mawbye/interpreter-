(define letTest
    (lambda (bool)
        (let ((x 3) (y 5))
            (let ((x y) (y x))
                (if bool x y)))))

(define letStarTest
    (lambda (bool)
        (let ((x 3) (y 5))
            (let* ((x y) (y x))
                (if bool x y)))))

"Should print 5 3:"
(letTest #t)
(letTest #f)

"Should print 5 5:"
(letStarTest #t)
(letStarTest #f)
        
